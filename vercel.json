{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "api/tripadvisor.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/tripadvisor", "dest": "/api/tripadvisor.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

